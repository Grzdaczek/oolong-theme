{
	"name": "Oolong",
	"type": "dark",
	"colors": {
		"focusBorder": "#0000",
		"foreground":"#ddd",
		
		"editor.background": "#2d2f32",
		"editor.lineHighlightBackground": "#ffe79210",
		"editor.selectionBackground": "#ffe79235",
		"editor.findMatchHighlightBackground": "#88102a70",
		"editor.findMatchBackground": "#88102a",
		"editor.wordHighlightBackground": "#fff2",
		"editor.wordHighlightStrongBackground": "#f7a21b70",
		"editor.hoverHighlightBackground": "#fff1",
		
		"sideBar.background": "#111",
		"sideBar.foreground": "#fff6",
		"sideBarSectionHeader.foreground": "#fff6",
		"sideBarSectionHeader.background": "#0000",
		"sideBarTitle.foreground": "#fff5",
		
		"gitDecoration.ignoredResourceForeground": "#fff2",
		"gitDecoration.untrackedResourceForeground": "#1bacb1",
		"gitDecoration.modifiedResourceForeground": "#f7a21b",
		"gitDecoration.submoduleResourceForeground": "#a948d6",
		"gitDecoration.renamedResourceForeground": "#fff",
		"gitDecoration.stageModifiedResourceForeground": "#fff",
		"gitDecoration.stageDeletedResourceForeground": "#fff",
		"gitDecoration.addedResourceForeground": "#fff",

		"activityBar.background": "#111",
		"activityBar.foreground": "#fff5",
		"activityBarBadge.background": "#88102a",
		"activityBarBadge.foreground": "#fffa",
		
		"tab.activeBackground": "#fff2",
		"tab.inactiveBackground": "#2d2f32",
		
		"editorGroupHeader.tabsBackground": "#2d2f32",
		"breadcrumb.background": "#fff2",
		
		"statusBar.background": "#111",
		"statusBar.foreground": "#fff5",
		"statusBarItem.hoverBackground": "#fff1",
		"statusBar.debuggingBackground": "#88102a",
		"statusBar.debuggingForeground": "#fff8",
		
		"notifications.background": "#111",
		
		"scrollbar.shadow": "#222",
		
		"panel.background": "#111",
		"panel.border": "#000",

		"terminal.foreground": "#aaa"
	},

	"tokenColors": [
		{
			"scope": [
			],
			"settings": {
				"foreground": "#bbbbbb",
			}
		},
		{
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00000088"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#81837a"
			}
		},
		{
			"scope": [
				"string",
				"constant.numeric",
				"constant.character",
				"constant.regexp",
				"constant.other.color",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6c7452"
			}
		},
		{
			"scope": [
				"meta.function.call entity.name.function",
				"meta.function-call entity.name.function",
			],
			"settings": {
				"foreground": "#f7a21b"
			}
		},

		{
			"scope": [
				"support.constant",
				"support.class",
				"support.type",
				"meta.use",
				"meta.import",
			],
			"settings": {
				"foreground": "#da8656",
			}
		},
		{
			"scope": [
				"keyword",
				"meta.function.definition entity.name.function",
			],
			"settings": {
				"foreground": "#f0624b"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type",
			],
			"settings": {
				"foreground": "#3d8e91"
			}
		},
		{
			"scope": [
				// "entity.name.type",
				// "entity.name.namespace",
			],
			"settings": {
				// "foreground": "#506a83",
				"fontStyle": "italic underline"
			}
		},
		// --------------------------------------------------------------------
		{
			"scope": [
				"constant.language",
				"markup.deleted",
			],
			"settings": {
				"foreground": "#e61f44"
			}
		},
		{
			"scope": [
				"meta.object-literal.key",
			],
			"settings": {
				"foreground": "#c4bc76",
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "",
				"foreground": "#e2e2e2"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "#664e4d",
				"foreground": "#cf433e",
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#664e4d",
				"foreground": "#cf433e"
			}
		},
		{
			"name": "diff.header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#534b4b"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#f7b83d"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#8FBE00A0"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "Function argument",
			"scope": "meta.property-value punctuation.separator.key-value",
			"settings": {
				"foreground": "#e2e2e2"
			}
		},
		{
			"name": "Markdown bold/italic",
			"scope": "markup.italic.markdown",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown bold/italic",
			"scope": "markup.bold.markdown",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown pre",
			"scope": "markup.raw.block.markdown",
			"settings": {
				"foreground": "#664e4d"
			}
		},
		{
			"name": "GitGutter deleted",
			"scope": "markup.deleted.git_gutter",
			"settings": {
				"foreground": "#e61f44"
			}
		},
		{
			"name": "GitGutter inserted",
			"scope": "markup.inserted.git_gutter",
			"settings": {
				"foreground": "#a7da1e"
			}
		},
		{
			"name": "GitGutter changed",
			"scope": "markup.changed.git_gutter",
			"settings": {
				"foreground": "#f7b83d"
			}
		},
		{
			"name": "Template expressions.",
			"scope": "meta.template.expression",
			"settings": {
				"foreground": "#e2e2e2"
			}
		}
	]
}
